{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "this is my custom Secret key for authentication",
    "Issuer": "your_app",
    "Audience": "your_app_users",
    "ExpiresInMinutes": 60
  },
  "Kafka": {
    "Mode": "Azure",
    "BootstrapServers": "mykafkahub.servicebus.windows.net:9093",
    "Topic": "orders",
    "ConsumerGroupId": "order-consumer-group"
  }
}
